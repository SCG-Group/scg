{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"settings": {
		"appearanceTools": false,
		"border": {
			"color": false,
			"radius": false,
			"style": false,
			"width": false
		},
		"color": {
			"text": true,
			"heading": true,
			"background": true,
			"link": true,
			"caption": true,
			"custom": false,
			"customDuotone": false,
			"customGradient": false,
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"duotone": [],
			"gradients": [],
			"palette": []
		},
		"custom": {},
		"dimensions": {
			"aspectRatio": false,
			"aspectRatios": [],
			"defaultAspectRatios": false,
			"minHeight": false
		},
		"layout": {
			"allowCustomContentAndWideSize": true,
			"allowEditing": true,
			"contentSize": "770px",
			"wideSize": "1170px"
		},
		"position": {
			"sticky": false
		},
		"shadow": {
			"defaultPresets": false,
			"presets": []
		},
		"spacing": {
			"blockGap": true,
			"customSpacingSize": false,
			"defaultSpacingSizes": false,
			"margin": false,
			"padding": false,
			"spacingScale": {
				"steps": 0
			},
			"spacingSizes": []
		},
		"typography": {
			"fluid": {
				"maxViewportWidth": "768px",
				"minViewportWidth": "375px"
			},
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Inter Tight",
							"fontStyle": "normal",
							"fontWeight": "400",
							"fontDisplay": "swap",
							"src": ["file:./assets/fonts/InterTight-VariableFont_wght.woff2"]
						},
						{
							"fontFamily": "Inter Tight",
							"fontStyle": "normal",
							"fontWeight": "700",
							"fontDisplay": "swap",
							"src": ["file:./assets/fonts/InterTight-VariableFont_wght.woff2"]
						}
					],
					"fontFamily": "Inter Tight, -apple-system, BlinkMacSystemFont, Segoe UI, Roboto, Helvetica Neue, Arial, sans-serif",
					"name": "Body",
					"slug": "body"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Inter Tight",
							"fontStyle": "normal",
							"fontWeight": "300",
							"fontDisplay": "swap",
							"src": ["file:./assets/fonts/InterTight-VariableFont_wght.woff2"]
						}
					],
					"fontFamily": "Inter Tight Light, -apple-system, BlinkMacSystemFont, Segoe UI, Roboto, Helvetica Neue, Arial, sans-serif",
					"name": "Lead",
					"slug": "lead"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Inter Tight",
							"fontStyle": "italic",
							"fontWeight": "800",
							"fontDisplay": "swap",
							"src": ["file:./assets/fonts/InterTight-ExtraBoldItalic.woff2"]
						}
					],
					"fontFamily": "Inter Tight Italic, -apple-system, BlinkMacSystemFont, Segoe UI, Roboto, Helvetica Neue, Arial, sans-serif",
					"name": "Headings",
					"slug": "headings"
				}
			],
			"customFontSize": false,
			"defaultFontSizes": false,
			"dropCap": false,
			"fontSizes": [],
			"fontStyle": false,
			"fontWeight": false,
			"letterSpacing": false,
			"lineHeight": false,
			"textAlign": true,
			"textColumns": false,
			"textDecoration": true,
			"textTransform": true,
			"writingMode": false
		},
		"useRootPaddingAwareAlignments": true,
		"blocks": {
			"scg/fullscreen-image": {
				"background": {
					"backgroundImage": true,
					"backgroundSize": true
				}
			}
		}
	},
	"styles": {
		"typography": {
			"fontFamily": "var:preset|font-family|body",
			"fontWeight": "400",
			"fontStyle": "normal",
			"lineHeight": "1.2"
		},
		"elements": {
			"heading": {
				"typography": {
					"fontFamily": "var:preset|font-family|headings",
					"fontWeight": "800",
					"fontStyle": "italic"
				}
			},
			"link": {
				"typography": {
					"textDecoration": "underline"
				}
			},
			"cite": {
				"typography": {
					"fontFamily": "var:preset|font-family|headings",
					"fontWeight": "800",
					"fontStyle": "italic"
				}
			}
		}
	},
	"customTemplates": [
		{
			"name": "page-with-cover",
			"postTypes": ["page"],
			"title": "Page with Cover Image"
		}
	],
	"templateParts": [
		{
			"name": "header",
			"title": "Site Header",
			"area": "header"
		},
		{
			"name": "footer",
			"title": "Site Footer",
			"area": "footer"
		},
		{
			"name": "social-media-links",
			"title": "Social Media Links",
			"area": "uncategorized"
		}
	]
}
