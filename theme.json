{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"settings": {
		"appearanceTools": false,
		"border": {
			"color": false,
			"radius": false,
			"style": false,
			"width": false
		},
		"color": {
			"text": true,
			"heading": true,
			"background": false,
			"link": false,
			"caption": false,
			"custom": false,
			"customDuotone": false,
			"customGradient": false,
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"duotone": [],
			"gradients": [],
			"palette": [
				{
					"color": "#415464",
					"name": "Cold grey",
					"slug": "base"
				},
				{
					"color": "#F9A51A",
					"name": "Yellow",
					"slug": "accent"
				},
				{
					"color": "#fff",
					"name": "White",
					"slug": "white"
				}
			]
		},
		"custom": {
			"border": {
				"base": "rgba(65, 84, 100, 0.25)"
			},
			"color": {
				"white-2": "rgba(255, 255, 255, 0.5)"
			},
			"transition": {
				"property": "color, background-color, border-color, box-shadow, opacity, transform",
				"duration": "0.35s",
				"easing": "ease-out"
			}
		},
		"dimensions": {
			"aspectRatio": false,
			"aspectRatios": [],
			"defaultAspectRatios": false,
			"minHeight": false
		},
		"layout": {
			"allowCustomContentAndWideSize": true,
			"allowEditing": true,
			"contentSize": "clamp(20.5rem, 20.5rem + ((100vw - 22.5rem) * 0.86087), 87.5vw)"
		},
		"position": {
			"sticky": false
		},
		"shadow": {
			"defaultPresets": false,
			"presets": []
		},
		"spacing": {
			"blockGap": true,
			"customSpacingSize": false,
			"defaultSpacingSizes": false,
			"margin": false,
			"padding": false,
			"spacingScale": {
				"steps": 0
			},
			"spacingSizes": [
				{
					"name": "128/64",
					"size": "clamp(4rem, 4rem + ((100vw - 22.5rem) * 0.06957), calc(1vw + 7.2rem))",
					"slug": "128"
				},
				{
					"name": "80",
					"size": "clamp(3.5rem, 3.5rem + ((100vw - 22.5rem) * 0.03479), calc(1vw + 4.2rem))",
					"slug": "80"
				},
				{
					"name": "64/32",
					"size": "clamp(2rem, 2rem + ((100vw - 22.5rem) * 0.03479), calc(1vw + 3.2rem))",
					"slug": "64"
				},
				{
					"name": "40",
					"size": "clamp(2.5rem, 2.5rem + ((100vw - 22.5rem) * 0.01547), calc(1vw + 1.7rem))",
					"slug": "40"
				},
				{
					"name": "32/24",
					"size": "clamp(1.5rem, 1.5rem + ((100vw - 22.5rem) * 0.00870), calc(1vw + 1.2rem))",
					"slug": "32"
				},
				{
					"name": "32",
					"size": "clamp(2rem, 2rem, calc(1vw + 1.2rem))",
					"slug": "32-fixed"
				},
				{
					"name": "24",
					"size": "clamp(1.5rem, 1.5rem, calc(1vw + 0.7rem))",
					"slug": "24"
				},
				{
					"name": "20",
					"size": "clamp(1.25rem, 1.25rem, calc(1vw + 0.45rem))",
					"slug": "20"
				},
				{
					"name": "16",
					"size": "1rem",
					"slug": "16"
				}
			]
		},
		"typography": {
			"fluid": false,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Inter Tight",
							"fontStyle": "normal",
							"fontWeight": "400",
							"fontDisplay": "swap",
							"src": [
								"file:./assets/fonts/InterTight-VariableFont_wght.woff2"
							]
						},
						{
							"fontFamily": "Inter Tight",
							"fontStyle": "normal",
							"fontWeight": "700",
							"fontDisplay": "swap",
							"src": [
								"file:./assets/fonts/InterTight-VariableFont_wght.woff2"
							]
						}
					],
					"fontFamily": "Inter Tight, -apple-system, BlinkMacSystemFont, Segoe UI, Roboto, Helvetica Neue, Arial, sans-serif",
					"name": "Body",
					"slug": "body"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Inter Tight",
							"fontStyle": "normal",
							"fontWeight": "300",
							"fontDisplay": "swap",
							"src": [
								"file:./assets/fonts/InterTight-VariableFont_wght.woff2"
							]
						}
					],
					"fontFamily": "Inter Tight Light, -apple-system, BlinkMacSystemFont, Segoe UI, Roboto, Helvetica Neue, Arial, sans-serif",
					"name": "Lead",
					"slug": "lead"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Inter Tight",
							"fontStyle": "italic",
							"fontWeight": "800",
							"fontDisplay": "swap",
							"src": [
								"file:./assets/fonts/InterTight-ExtraBoldItalic.woff2"
							]
						}
					],
					"fontFamily": "Inter Tight Italic, -apple-system, BlinkMacSystemFont, Segoe UI, Roboto, Helvetica Neue, Arial, sans-serif",
					"name": "Headings",
					"slug": "headings"
				}
			],
			"customFontSize": false,
			"defaultFontSizes": false,
			"dropCap": false,
			"fontSizes": [
				{
					"name": "200/128",
					"size": "clamp(4.25rem, 4.25rem + ((100vw - 22.5rem) * 0.07964), calc(1vw + 11.7rem))",
					"slug": "200"
				},
				{
					"name": "160/96",
					"size": "clamp(4rem, 4rem + ((100vw - 22.5rem) * 0.06957), calc(1vw + 9.2rem))",
					"slug": "160"
				},
				{
					"name": "128/80",
					"size": "clamp(3.75rem, 3.75rem + ((100vw - 22.5rem) * 0.04248), calc(1vw + 7.2rem))",
					"slug": "128"
				},
				{
					"name": "96/64",
					"size": "clamp(3.5rem, 3.5em + ((100vw - 22.5rem) * 0.03536), calc(1vw + 5.2rem))",
					"slug": "96"
				},
				{
					"name": "80/56",
					"size": "clamp(3.25rem, 3.25rem + ((100vw - 22.5rem) * 0.02652), calc(1vw + 4.2rem))",
					"slug": "80"
				},
				{
					"name": "64/48",
					"size": "clamp(3rem, 3rem + ((100vw - 22.5rem) * 0.01768), calc(1vw + 3.2rem))",
					"slug": "64"
				},
				{
					"name": "56/40",
					"size": "clamp(2.5rem, 2.5rem + ((100vw - 22.5rem) * 0.01768), calc(1vw + 2.7rem))",
					"slug": "56"
				},
				{
					"name": "48/32",
					"size": "clamp(2rem, 2rem + ((100vw - 22.5rem) * 0.01768), calc(1vw + 2.2rem))",
					"slug": "48"
				},
				{
					"name": "40/24",
					"size": "clamp(1.5rem, 1.5rem + ((100vw - 22.5rem) * 0.01768), calc(1vw + 1.7rem))",
					"slug": "40"
				},
				{
					"name": "32/24",
					"size": "clamp(1.5rem, 1.5rem + ((100vw - 22.5rem) * 0.00884), calc(1vw + 1.2rem))",
					"slug": "32"
				},
				{
					"name": "32/20",
					"size": "clamp(1.25rem, 1.25rem + ((100vw - 22.5rem) * 0.01326), calc(1vw + 1.2rem))",
					"slug": "32-alt"
				},
				{
					"name": "24/18",
					"size": "clamp(1.125rem, 1.125rem + ((100vw - 22.5rem) * 0.00663), calc(1vw + 0.7rem))",
					"slug": "24"
				},
				{
					"name": "18/16",
					"size": "clamp(1rem, 1rem + ((100vw - 22.5rem) * 0.004), calc(1vw + 0.325rem))",
					"slug": "18"
				},
				{
					"name": "16",
					"size": "clamp(1rem, 1rem + ((100vw - 22.5rem) * 0.00221), calc(1vw + 0.2rem))",
					"slug": "16"
				},
				{
					"name": "14",
					"size": "clamp(0.875rem, 0.875rem + ((100vw - 22.5rem) * 0.00221), calc(1vw + 0.075rem))",
					"slug": "14"
				}
			],
			"fontStyle": false,
			"fontWeight": true,
			"letterSpacing": false,
			"lineHeight": false,
			"textAlign": true,
			"textColumns": false,
			"textDecoration": false,
			"textTransform": true,
			"writingMode": false
		},
		"useRootPaddingAwareAlignments": false,
		"blocks": {
			"core/group": {
				"spacing": {
					"padding": true,
					"margin": true
				},
				"color": {
					"background": true
				}
			}
		}
	},
	"styles": {
		"typography": {
			"fontFamily": "var:preset|font-family|body",
			"fontWeight": "400",
			"fontStyle": "normal",
			"lineHeight": "1.2",
			"fontSize": "var:preset|font-size|18"
		},
		"color": {
			"text": "var:preset|color|base"
		},
		"spacing": {
			"blockGap": "var:preset|spacing|32"
		},
		"elements": {
			"heading": {
				"typography": {
					"fontFamily": "var:preset|font-family|headings",
					"fontWeight": "800",
					"fontStyle": "italic"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var:preset|font-size|96",
					"lineHeight": "1"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var:preset|font-size|56"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var:preset|font-size|48"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var:preset|font-size|32",
					"fontFamily": "var:preset|font-family|body",
					"fontWeight": "700",
					"fontStyle": "normal"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var:preset|font-size|24",
					"fontFamily": "var:preset|font-family|body",
					"fontWeight": "700",
					"fontStyle": "normal"
				}
			},
			"link": {
				"typography": {
					"textDecoration": "underline"
				},
				"color": {
					"text": "currentColor"
				},
				"css": "text-underline-offset: 0.2em; &:hover { color: var(--wp--preset--color--accent); }"
			},
			"cite": {
				"typography": {
					"fontFamily": "var:preset|font-family|headings",
					"fontWeight": "800",
					"fontStyle": "italic"
				}
			}
		},
		"blocks": {
			"core/list": {
				"spacing": {
					"padding": {
						"left": "1.65em"
					}
				},
				"css": "&:is(ul) { list-style-type: disc; }"
			},
			"core/social-links": {
				"spacing": {
					"blockGap": "var:preset|spacing|16"
				}
			},
			"core/navigation-link": {
				"css": "&:not(.is-style-main) a { text-decoration: underline; text-underline-offset: 0.2em; }"
			},
			"core/post-excerpt": {
				"typography": {
					"fontFamily": "var:preset|font-family|lead",
					"fontSize": "var:preset|font-size|32"
				}
			},
			"core/spacer": {
				"color": {
					"background": "var:custom|border|base"
				}
			}
		}
	},
	"customTemplates": [
		{
			"name": "page-no-title",
			"postTypes": [ "page" ],
			"title": "One-page"
		}
	],
	"templateParts": [
		{
			"name": "header",
			"title": "Site Header",
			"area": "header"
		},
		{
			"name": "header-one-page",
			"title": "Site Header (One Page)",
			"area": "header"
		},
		{
			"name": "footer",
			"title": "Site Footer",
			"area": "footer"
		},
		{
			"name": "social-media-links",
			"title": "Social Media Links",
			"area": "uncategorized"
		}
	]
}
