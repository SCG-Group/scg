{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "scg/contact",
	"version": "0.1.0",
	"title": "SCG Contact Info",
	"category": "scg",
	"icon": "location",
	"description": "Block with map and contact details.",
	"example": {},
	"attributes": {
		"lat": {
			"type": "number"
		},
		"lng": {
			"type": "number"
		}
	},
	"supports": {
		"interactivity": true,
		"alignWide": true,
		"align": true
	},
	"textdomain": "scg",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScriptModule": "file:./view.ts"
}
