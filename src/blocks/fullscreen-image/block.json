{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "scg/fullscreen-image",
	"version": "0.1.0",
	"title": "SCG Fullscreen Image",
	"category": "scg",
	"icon": "smiley",
	"description": "Fullscreen block with fixed background image.",
	"example": {},
	"attributes": {
		"style": {
			"type": "object",
			"default": {
				"background": {
					"backgroundSize": "cover",
					"backgroundAttachment": "fixed"
				}
			}
		}
	},
	"supports": {
		"html": false,
		"background": {
			"backgroundImage": true,
			"backgroundSize": true
		}
	},
	"textdomain": "scg",
	"editorScript": "file:./index.js",
	"style": "file:./style-index.css"
}
