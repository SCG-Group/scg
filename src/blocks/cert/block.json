{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "scg/cert",
	"version": "0.1.0",
	"title": "SCG Certificate",
	"category": "scg",
	"description": "Information about certificate and ability to display PDF.",
	"example": {
		"attributes": {
			"name": "ISO 9001:2015",
			"category": "Security",
			"certId": 123,
			"img": "/wp-content/themes/scg/assets/images/sample-cert.webp"
		},
		"viewportWidth": 1000
	},
	"icon": "star-empty",
	"attributes": {
		"name": {
			"type": "string",
			"default": "ISO"
		},
		"category": {
			"type": "string"
		},
		"img": {
			"type": "string"
		},
		"imgId": {
			"type": "number"
		},
		"cert": {
			"type": "string"
		},
		"certId": {
			"type": "number"
		},
		"certFilename": {
			"type": "string"
		}
	},
	"supports": {
		"interactivity": true
	},
	"textdomain": "scg",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"render": "file:./render.php",
	"style": "file:./style-index.css"
}
