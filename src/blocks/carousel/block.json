{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "scg/carousel",
	"version": "0.1.0",
	"title": "SCG Carousel",
	"category": "scg",
	"description": "Image carousel block.",
	"example": {
		"innerBlocks": [
			{
				"name": "core/image",
				"attributes": {
					"url": "/wp-content/themes/scg/assets/images/sample-image.webp",
					"sizeSlug": "thumbnail"
				}
			},
			{
				"name": "core/image",
				"attributes": {
					"url": "/wp-content/themes/scg/assets/images/sample-image.webp",
					"sizeSlug": "thumbnail"
				}
			},
			{
				"name": "core/image",
				"attributes": {
					"url": "/wp-content/themes/scg/assets/images/sample-image.webp",
					"sizeSlug": "thumbnail"
				}
			},
			{
				"name": "core/image",
				"attributes": {
					"url": "/wp-content/themes/scg/assets/images/sample-image.webp",
					"sizeSlug": "thumbnail"
				}
			}
		]
	},
	"allowedBlocks": [ "core/image" ],
	"attributes": {
		"spacing": {
			"type": "object",
			"blockGap": {
				"default": "var:style|block-gap"
			}
		}
	},
	"supports": {
		"html": false,
		"align": [ "wide", "full" ],
		"spacing": {
			"blockGap": true
		},
		"layout": {
			"allowEditing": false,
			"default": {
				"type": "flex",
				"flexWrap": "nowrap"
			}
		}
	},
	"selectors": {
		"layout": ".wp-block-scg-carousel__wrapper",
		"spacing": {
			"blockGap": ".wp-block-scg-carousel__wrapper"
		}
	},
	"textdomain": "scg",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScriptModule": "file:./view.js"
}
